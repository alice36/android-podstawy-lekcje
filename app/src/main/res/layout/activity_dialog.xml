<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="pl.javastart.ap.intent.IntentExampleActivity">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Info dialog"
        android:id="@+id/button10"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="infoDialog" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Pojedynczy element z listy"
        android:id="@+id/button11"
        android:onClick="singleListElementDialog"
        android:layout_below="@+id/button10"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Kilka elementÃ³w z listy"
        android:id="@+id/button12"
        android:onClick="multipleListElementsDialog"
        android:layout_below="@+id/button11"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Godzina"
        android:id="@+id/button13"
        android:onClick="hourSelectionDialog"
        android:layout_below="@+id/button12"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Data"
        android:id="@+id/button14"
        android:onClick="dateSelectionDialog"
        android:layout_below="@+id/button13"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Data i godzina"
        android:id="@+id/button15"
        android:onClick="hourAndHourSelectionDialog"
        android:layout_below="@+id/button14"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
